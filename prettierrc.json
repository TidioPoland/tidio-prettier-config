{
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "importOrder": [
      "(./i18n|./i18n/index)$",
      "^react(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^../shared/helpers/env$",
      "^@tidio/(.*)$",
      "^(store|store/(.*))$",
      "^(utils|utils(.*))$",
      "^components/(.*)(?<!gif|png|jpg|jpeg|svg|mp4|webm)$",
      "^(apiData|(apiData/(.*?))|helpers|(helpers/(.*?))|hooks|(hooks/(.*?))|design|(design/(.*?))|lang|logger|track|swal|upload|upgradeHelpers|chatData)$",
      "^((.*)/use[A-Z](.*?)|(.*)/hooks(.*?))$",
      "^[./](.*?)(shared|helpers|panelStore|loginStore)(.*?)(?<!gif|png|jpg|jpeg|svg|mp4|webm|less)$",
      "^[./](.*?)(?<!gif|png|jpg|jpeg|svg|mp4|webm|less)$",
      "^[./](.*?).less$",
      "^[./](.*?).(gif|png|jpg|jpeg|svg|mp4|webm)$",
      "^components/(.*).(gif|png|jpg|jpeg|svg|mp4|webm)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "overrides": [
        {
            "files": "*.ts",
            "options": {
                "importOrderParserPlugins": ["typescript"]
            }
        }
    ]
}
